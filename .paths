#!/usr/bin/env zsh

export PATH=$(brew --prefix coreutils)/libexec/gnubin:${PATH}
export PATH=$(brew --prefix findutils)/libexec/gnubin:${PATH}
export PATH=${PATH}:~/source/tools/bin
export CODE_DIR=~/source

export GOPATH=~/go
export PATH=$PATH:$GOPATH/bin

#export BUILD_TOOLS_PATH=${CODE_DIR}/FerryIslands/build-tools
export BUILD_TOOLS_PATH=${CODE_DIR}/FerryIslands/tm-build-tools

export PATH=${PATH}:${BUILD_TOOLS_PATH}

if type kubectl > /dev/null ; then
	source <(kubectl completion zsh)
fi
